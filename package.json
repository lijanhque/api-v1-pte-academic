{
  "private": true,
  "name": "nextjs",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=18.18",
    "pnpm": ">=10.23.0"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next dev --webpack",
    "dev:turbo": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next dev --turbopack",
    "dev:webpack": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next dev",
    "dev:debug": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --inspect\" next dev --turbopack",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "build:analyze": "cross-env ANALYZE=true NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "build:production": "pnpm clean && cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "start": "next start",
    "start:prod": "cross-env NODE_ENV=production next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next .turbo .swc node_modules/.cache",
    "clean:hard": "rimraf .next .turbo .swc node_modules/.cache && pnpm store prune",
    "clean:all": "rimraf .next .turbo .swc node_modules/.cache node_modules && pnpm install --force",
    "clean:windows": "powershell -ExecutionPolicy Bypass -File cleanup.ps1",
    "reinstall": "pnpm clean:all",
    "fresh": "pnpm clean && pnpm dev",
    "fresh:build": "pnpm clean && pnpm build",
    "db:seed": "tsx lib/db/seed.ts",
    "db:seed:speaking": "tsx lib/db/seed.ts --speaking --reset",
    "db:seed:all": "tsx lib/db/seed.ts --all --reset",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:test": "tsx test-db.ts",
    "prepare:assets": "node -e \"const fs=require('fs');const src='asset';const dst='public/asset';if(fs.existsSync(src)){fs.cpSync(src,dst,{recursive:true,force:true});console.log('Mirrored asset/ -> public/asset');}else{console.log('No asset/ dir present; skipping');}\"",
    "test": "echo 'No tests configured yet'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "deploy:check": "tsx scripts/pre-deploy-check.ts",
    "deploy:test": "tsx scripts/test-apis.ts",
    "deploy:vercel": "pnpm deploy:check && vercel --prod",
    "deploy:railway": "pnpm deploy:check && railway up",
    "postbuild": "echo '✅ Build completed successfully!'",
    "postinstall": "echo '✅ Dependencies installed successfully!'"
  },
  "dependencies": {
    "@ai-sdk/assemblyai": "^1.0.20",
    "@ai-sdk/google": "^2.0.38",
    "@ai-sdk/groq": "^2.0.31",
    "@ai-sdk/openai": "^2.0.65",
    "@ai-sdk/react": "^2.0.93",
    "@genkit-ai/mcp": "^1.23.0",
    "@google/genai": "^1.30.0",
    "@neondatabase/serverless": "^1.0.2",
    "@polar-sh/nextjs": "^0.7.0",
    "@polar-sh/sdk": "^0.41.4",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rollbar/react": "^1.0.0",
    "@tabler/icons-react": "^3.35.0",
    "@vercel/blob": "^2.0.0",
    "@vercel/toolbar": "^0.1.41",
    "ai": "^5.0.93",
    "assemblyai": "^4.19.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.32",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "framer-motion": "^12.23.24",
    "jose": "^6.0.11",
    "lucide-react": "^0.511.0",
    "mime-types": "^2.1.35",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.3",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.3",
    "postgres": "^3.4.5",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "radix-ui": "^1.4.2",
    "react": "19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.0",
    "recharts": "^3.4.1",
    "resend": "^6.4.2",
    "rollbar": "^2.26.5",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@next/bundle-analyzer": "^16.0.3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "4.1.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest": "^30.0.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^22.15.18",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.2.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "4.1.7",
    "tsx": "^4.19.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.47.0"
  }
}
