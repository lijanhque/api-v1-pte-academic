{
  "private": true,
  "name": "nextjs",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=18.18",
    "pnpm": ">=10.23.0"
  },
  "scripts": {
    "dev": "next dev --webpack",
    "dev:vm": "NODE_OPTIONS='--max-old-space-size=2048' next dev --webpack",
    "build": "next build",
    "build:vm": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "clean": "rimraf .next .turbo .swc node_modules/.cache",
    "clean:vm": "rimraf .next .turbo .swc node_modules/.cache pnpm-store",
    "clean:all": "rimraf .next .turbo .swc node_modules/.cache node_modules && pnpm install",
    "fresh": "pnpm clean && pnpm dev",
    "fresh:vm": "pnpm clean:vm && pnpm install:vm && pnpm dev:vm",
    "install:vm": "pnpm install --prefer-offline --child-concurrency 3",
    "db:seed": "npx tsx lib/db/seed.ts",
    "db:seed:speaking": "tsx lib/db/seed.ts --speaking --reset",
    "db:seed:all": "tsx lib/db/seed.ts --all --reset",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:test": "npx tsx test-db.ts",
    "prepare:assets": "node -e \"const fs=require('fs');const src='asset';const dst='public/asset';if(fs.existsSync(src)){fs.cpSync(src,dst,{recursive:true,force:true});console.log('Mirrored asset/ -> public/asset');}else{console.log('No asset/ dir present; skipping');}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "env:setup": "cp .env.example .env.local && echo '\u2705 Created .env.local from .env.example - Please update with your values'",
    "env:check": "node -e \"require('dotenv').config({path:'.env.local'});const required=['DATABASE_URL','BETTER_AUTH_SECRET','GOOGLE_GENERATIVE_AI_API_KEY'];const missing=required.filter(k=>!process.env[k]);if(missing.length){console.error('\u274c Missing required env vars:',missing.join(', '));process.exit(1)}else{console.log('\u2705 All required environment variables are set')}\"",
    "deploy:check": "tsx scripts/pre-deploy-check.ts",
    "deploy:test": "tsx scripts/test-apis.ts",
    "deploy:vercel": "pnpm deploy:check && vercel --prod",
    "deploy:railway": "pnpm deploy:check && railway up",
    "postbuild": "echo '\u2705 Build completed successfully!'",
    "vm:setup": "echo 'Setting up for VM development...' && pnpm install:vm && echo 'VM setup complete!'",
    "vm:dev": "pnpm fresh:vm",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.38",
    "@ai-sdk/groq": "^2.0.31",
    "@ai-sdk/openai": "^2.0.65",
    "@ai-sdk/react": "^2.0.93",
    "@genkit-ai/mcp": "^1.23.0",
    "@google/genai": "^1.30.0",
    "@neondatabase/serverless": "^1.0.2",
    "@polar-sh/nextjs": "^0.7.0",
    "@polar-sh/sdk": "^0.41.4",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rollbar/react": "^1.0.0",
    "@tabler/icons-react": "^3.35.0",
    "@vercel/blob": "^2.0.0",
    "@vercel/toolbar": "^0.1.41",
    "ai": "^5.0.93",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.32",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "framer-motion": "^12.23.24",
    "jose": "^6.0.11",
    "lucide-react": "^0.511.0",
    "mime-types": "^2.1.35",
    "next": "16.0.3",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.3",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.3",
    "postgres": "^3.4.5",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "radix-ui": "^1.4.2",
    "react": "19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.0",
    "recharts": "^3.4.1",
    "resend": "^6.4.2",
    "rollbar": "^2.26.5",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "4.1.7",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest": "^30.0.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.2.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "4.1.7",
    "tsx": "^4.19.1",
    "typescript": "^5",
    "typescript-eslint": "^8.47.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "jest": "^30.0.0-rc.0",
    "jest-environment-jsdom": "^30.0.0-rc.0"
  }
}
